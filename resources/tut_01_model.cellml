<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#"
  xmlns:cellml="http://www.cellml.org/cellml/2.0#" name="tut_01_model" id="tut_01_model_id_is_this">
  <component name="i_am_a_component" id="my_component_id">
    <variable units="dimensionless" name="a" initial_value="b" interface="public_and_private" />
    <variable units="dimensionless" name="b" interface="public_and_private" />
    <variable units="dimensionless" name="c" interface="public_and_private" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <ci>a</ci>
        <apply>
          <plus/>
          <ci>b</ci>
          <ci>c</ci>
        </apply>
      </apply>
    </math>
  </component>
</model>
